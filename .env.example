# نمونه فایل تنظیمات .env
# مقادیر را طبق ربات خود تکمیل کنید و فایل نهایی را به نام `.env` ذخیره کنید.

BOT_TOKEN=
BOT_USERNAME=
DB_PATH=data/app.db
LOG_LEVEL=INFO
LOG_TO_CONSOLE=false

# در صورت نیاز به سرور جایگزین Bot API
TELEGRAM_API_BASE=
TELEGRAM_FILE_API_BASE=

# در صورت نیاز به پراکسی (http/https/socks5)
TELEGRAM_PROXY=

# تنظیمات زمانی (بر حسب ثانیه)
TELEGRAM_REQUEST_TIMEOUT=60
TELEGRAM_RETRY_DELAY=5
